{
  "$schema": "https://json-schema.org/draft-04/schema#",
  "name": "laclique/andcollecte",
  "type": "project",
  "license": "MIT",
  "description": "WordPress with dependency injection",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "keywords": [
    "wordpress, slim, typescript"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/installers": "^1.8",
    "vlucas/phpdotenv": "^4.1.3",
    "oscarotero/env": "^1.2.0",
    "roots/wordpress": "5.6",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/wp-api-swaggerui": "^1.0",
    "ext-json": "*",
    "wpackagist-plugin/advanced-custom-fields": "^5.8",
    "wpackagist-plugin/wordpress-seo": "^14.5",
    "wpackagist-plugin/woocommerce": ">=5",
    "slim/slim": "^4.7",
    "slim/psr7": "^1.1",
    "nyholm/psr7": "^1.3",
    "nyholm/psr7-server": "^1.0",
    "guzzlehttp/psr7": "^1.6",
    "http-interop/http-factory-guzzle": "^1.0",
    "laminas/laminas-diactoros": "^2.3",
    "php-di/php-di": "^6.2",
    "php-di/slim-bridge": "^3.0",
    "wpackagist-plugin/elementor": "^3.0",
    "wpackagist-plugin/loco-translate": "^2.4",
    "wordplate/acf": "^9.0",
    "myclabs/php-enum": "^1.8"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.4",
    "roave/security-advisories": "dev-master",
    "phpstan/phpstan": "^0.12.32",
    "symfony/var-dumper": "^5.1",
    "phpunit/phpunit": "^8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
