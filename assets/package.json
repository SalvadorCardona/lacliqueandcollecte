{
  "name": "lacliqueandcollecte",
  "version": "1.0.0",
  "description": "Wordpress Api Rest With Slim framework and angular theme",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "parcel": "parcel ./index.html  --out-dir ./../web/app/themes/hello-theme-master/dist -o app",
    "parcel-build": "parcel build ./index.html  -d ./../web/app/themes/hello-theme-master/dist -o app",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvadorCardona/lacliqueandcollecte.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SalvadorCardona/lacliqueandcollecte/issues"
  },
  "homepage": "https://github.com/SalvadorCardona/lacliqueandcollecte#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.9",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@types/node": "^14.14.10",
    "app": "^0.1.0",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "browserslist": "^4.15.0",
    "custom-elements-ts": "^0.0.16"
  },
  "resolutions": {
    "browserslist": "4.14.2",
    "caniuse-lite": "1.0.30001129"
  },
  "alias": {
    "App": "./src"
  }
}
